<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chai.inv.LotMasterListController">
   <center>
      <TableView fx:id="lotMasterTable" prefHeight="413.0" prefWidth="900.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="lotNumberColumn" prefWidth="91.0" text="Lot #" />
          <TableColumn fx:id="warehouseColumn" prefWidth="86.0" text="Warehouse" />
            <TableColumn fx:id="itemNumberColumn" prefWidth="86.0" text="Item#" />
            <TableColumn fx:id="itemDescriptionColumn" prefWidth="86.0" text="Item Description" />
            <TableColumn fx:id="selfLifeColumn" prefWidth="91.0" text="Shelf Life" />
            <TableColumn fx:id="mfgDateColumn" prefWidth="91.0" text="Mfg. Date" />
            <TableColumn fx:id="expirationDateColumn" prefWidth="91.0" text="Expiration Date" />
            <TableColumn fx:id="startDateColumn" prefWidth="91.0" text="Start Date" />
            <TableColumn fx:id="endDateColumn" prefWidth="91.0" text="End Date" />
            <TableColumn fx:id="statusColumn" prefWidth="91.0" text="Status" />
            <TableColumn fx:id="itemIdColumn" prefWidth="91.0" text="ItemId" visible="false" />
            <TableColumn fx:id="warehouseIdColumn" prefWidth="91.0" text="WarehouseId" visible="false" />
        </columns>
      </TableView>
   </center>
   <top>
      <GridPane prefHeight="87.0" prefWidth="900.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Item Lots">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Label text="Item" textFill="#171986" GridPane.rowIndex="1" fx:id="x_LOT_ITEM_NAME">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Label>
         </children>
      </GridPane>
   </top>
</BorderPane>
