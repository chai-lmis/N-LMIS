<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chai.inv.MonthlyTransactionSummaryController">
   <top>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Label text="Monthly Transaction Summary">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
               <VBox.margin>
                  <Insets left="510.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
   </top>
   <center>
      <VBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane prefHeight="47.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="191.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="501.0" minWidth="10.0" prefWidth="319.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="514.0" minWidth="10.0" prefWidth="129.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="537.0" minWidth="10.0" prefWidth="118.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="537.0" minWidth="10.0" prefWidth="386.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <ComboBox editable="true" onAction="#changeItemHandle" promptText="Select Product" visibleRowCount="6" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_ITEM">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </ComboBox>
                  <Label text="Product:" GridPane.valignment="BOTTOM">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#handleGetAnalysisList" prefHeight="30.0" prefWidth="82.0" text="Refresh" GridPane.columnIndex="3" GridPane.rowIndex="1" GridPane.valignment="BASELINE">
                     <GridPane.margin>
                        <Insets left="10.0" />
                     </GridPane.margin>
                  </Button>
                  <Label layoutX="20.0" layoutY="15.0" text="Period:" GridPane.columnIndex="2" GridPane.valignment="BOTTOM">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <ComboBox prefWidth="150.0" promptText="Select Month" visibleRowCount="6" GridPane.columnIndex="2" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_PERIOD" />
                  <Label text="Product Description" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="BASELINE" fx:id="x_ITEM_DESCRIPTION">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <GridPane.margin>
                        <Insets left="5.0" />
                     </GridPane.margin>
                  </Label>
               </children>
            </GridPane>
            <TableView fx:id="x_MONTHLY_TRANSACTION_SUMMARY_TABLE">
              <columns>
                <TableColumn prefWidth="75.0" text="Product" fx:id="x_ITEM_COLUMN" />
                <TableColumn prefWidth="75.0" text="UOM" fx:id="x_UOM_COLUMN" />
                  <TableColumn prefWidth="75.0" text="Lot#" fx:id="x_LOT_COLUMN" />
                  <TableColumn prefWidth="110.0" style="-fx-alignment: CENTER-RIGHT;" text="Opening Balance" fx:id="x_OPENING_COLUMN" />
                  <TableColumn prefWidth="75.0" style="-fx-alignment: CENTER-RIGHT;" text="Receipt" fx:id="x_RECEIPT_COLUMN" />
                  <TableColumn prefWidth="75.0" text="Issue" fx:id="x_ISSUE_COLUMN" />
                  <TableColumn prefWidth="75.0" style="-fx-alignment: CENTER-RIGHT;" text="Calculated" fx:id="x_CALCULATED_COLUMN" />
                  <TableColumn prefWidth="105.0" style="-fx-alignment: CENTER-RIGHT;" text="Ending Balance" fx:id="x_ACTUAL_COLUMN" />
                  <TableColumn prefWidth="75.0" style="-fx-alignment: CENTER-RIGHT;" text="Delta" fx:id="x_DELTA_COLUMN" />
              </columns>
            </TableView>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox prefHeight="62.0" prefWidth="1143.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleHomeDashBoardBtn" prefHeight="45.0" prefWidth="50.0" text="Home">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/home.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleBackToReportsDashboard" prefHeight="45.0" prefWidth="50.0" text="Back">
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/Back.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
         <padding>
            <Insets left="20.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
