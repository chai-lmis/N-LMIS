<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="607.0" prefWidth="1296.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chai.inv.VendorMainController">
   <top>
      <GridPane>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <HBox prefHeight="61.0" prefWidth="696.0" spacing="3.0" fx:id="x_HBOX">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleAddAction" text="Add" fx:id="x_ADD_VENDOR_BTN">
                     <graphic>
                        <ImageView fitHeight="23.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../resources/icons/file_add.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="25.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleEditAction" text="Edit">
                     <graphic>
                        <ImageView fitHeight="23.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../resources/icons/file_edit.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="105.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleSearchAction" text="Search">
                     <graphic>
                        <ImageView fitHeight="23.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../resources/icons/file_search.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="185.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleHistoryAction" text="History">
                     <graphic>
                        <ImageView fitHeight="23.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../resources/icons/file_history.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button layoutX="265.0" layoutY="25.0" mnemonicParsing="false" onAction="#handleExportAction" text="Export">
                     <graphic>
                        <ImageView fitHeight="23.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../../../../resources/icons/Export_load_upload.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <padding>
                  <Insets left="6.0" top="15.0" />
               </padding>
            </HBox>
         </children>
      </GridPane>
   </top>
   <center>
      <TableView fx:id="vendorTable" prefHeight="537.0" prefWidth="1312.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="vendorNumberColumn" prefWidth="164.0" text="Vendor Number" />
            <TableColumn fx:id="vendorNameColumn" minWidth="0.0" prefWidth="122.0" text="Vendor Name" />
            <TableColumn fx:id="descriptonColumn" prefWidth="54.0" text="Description" />
            <TableColumn fx:id="addressColumn" prefWidth="112.34423828125" text="Address" />
            <TableColumn fx:id="cityColumn" prefWidth="68.779296875" text="City" />
            <TableColumn fx:id="stateColumn" prefWidth="55.0" text="State" />
            <TableColumn fx:id="countryColumn" prefWidth="91.0" text="Country" />
            <TableColumn fx:id="zipcodeColumn" prefWidth="91.0" text="Zip Code" />
            <TableColumn fx:id="telephoneColumn" prefWidth="91.0" text="Telephone" />
            <TableColumn fx:id="faxColumn" prefWidth="91.0" text="Fax" visible="false" />
            <TableColumn fx:id="emailColumn" prefWidth="75.0" text="E-Mail" />
            <TableColumn fx:id="statusColumn" prefWidth="91.0" text="Status" />
            <TableColumn fx:id="startDateColumn" prefWidth="91.0" text="Start Date" />
            <TableColumn fx:id="endDateColumn" prefWidth="91.0" text="End Date" />
            <TableColumn fx:id="companyIdColumn" prefWidth="91.0" text="Company Id" visible="false" />
            <TableColumn fx:id="countryIdColumn" prefWidth="91.0" text="Country Id" visible="false" />
            <TableColumn fx:id="stateIdColumn" prefWidth="91.0" text="State Id" visible="false" />
            <TableColumn fx:id="cityIdColumn" prefWidth="91.0" text="City Id" visible="false" />
            <TableColumn fx:id="vendorIdColumn" prefWidth="103.79290771484375" text="Vendor Id" visible="false" />
         </columns>
      </TableView>
   </center>
   <bottom>
      <HBox prefHeight="68.0" prefWidth="900.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleHomeDashBoardBtn" prefHeight="44.0" prefWidth="61.0" text="Home">
               <graphic>
                  <ImageView fitHeight="33.0" fitWidth="37.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/home.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
            <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#handleBackToMaintenanceSubMenu" prefHeight="44.0" prefWidth="60.0" text="Back">
               <graphic>
                  <ImageView fitHeight="25.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../../resources/icons/Back.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children>
         <padding>
            <Insets left="30.0" top="10.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
