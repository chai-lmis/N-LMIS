                        <MenuItem mnemonicParsing="false" onAction="#handleExitMenuAction" text="Exit" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Stock Management" fx:id="x_STOCK_MANAGEMENT_MENU">
